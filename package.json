  "keywords": [
    "jingle",
    "webrtc",
    "xmpp",
    "electron",
    "jitsi-meet"
  ],
  "author": "",
  "readmeFilename": "README.md",
  "license": "Apache-2.0",
  "dependencies": {
    "robotjs": "hristoterezov/robotjs",
    "postis": "^2.2.0"
  },
  "devDependencies": {
    "electron": "1.4.13",
    "electron-packager": "*",
    "electron-rebuild": "^1.5.6",
    "eslint": ">=3",
    "eslint-plugin-jsdoc": "*",
    "precommit-hook": "3.0.0"
  }
}
