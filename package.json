{
  "name": "hopp-digitales-klassenzimmer",
  "version": "2022.6.1",
  "description": "HOPP Foundation - Das digitale Klassenzimmer",
  "main": "./build/main.js",
  "productName": "Digitales Klassenzimmer",
  "scripts": {
    "start": "webpack --config ./webpack.main.js --mode development && concurrently \"npm:watch\" \"electron ./build/main.js\"",
    "clean": "rm -rf node_modules build dist",
    "lint": "eslint .",
    "build": "webpack --config ./webpack.main.js --mode production && webpack --config ./webpack.renderer.js --mode production",
    "dist": "npm run build && electron-builder",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "watch": "webpack --config ./webpack.renderer.js --mode development --watch --watch-options-poll=1000"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "build": {
    "appId": "de.hopp-foundation.klassenzimmer",
    "productName": "Digitales Klassenzimmer",
    "generateUpdatesFilesForAllChannels": true,
    "afterSign": "./notarize.js",
    "files": [
      "build",
      "resources",
      "!app",
      "!main.js",
      "app/main.css",
      "app/onboarding.css",
      "app/images"
    ],
    "mac": {
      "artifactName": "digitales-klassenzimmer-${version}.${ext}",
      "target": [
        {
          "arch": "universal",
          "target": "mas"
        }
      ],
      "category": "public.app-category.video",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "electronLanguages": [
        "de",
        "en"
      ],
      "asarUnpack": "**/*.node",
      "extendInfo": {
        "NSCameraUsageDescription": "Digitales Klassenzimmer benötigt Zugriff auf eine Kamera für Videoanrufe",
        "NSMicrophoneUsageDescription": "Digitales Klassenzimmer bentötigt Zugriff auf ein Mikrofon um Audio/Video Anrufe zu tätigen.",
        "LSMultipleInstancesProhibited": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "mas": {
      "provisioningProfile": "embedded.provisionprofile",
      "entitlements": "resources/entitlements.mas.plist",
      "entitlementsInherit": "resources/entitlements.mas.inherit.plist",
      "hardenedRuntime": false
    },
    "linux": {
      "artifactName": "digitales-klassenzimmer-${arch}.${ext}",
      "category": "VideoConference;AudioVideo;Audio;Video;Network",
      "description": "HOPP Foundation - Digitales Klassenzimmer",
      "executableName": "digitales-klassenzimmer",
      "target": [
        {
          "arch": "x64",
          "target": "AppImage"
        },
        {
          "arch": "x64",
          "target": "snap"
        }
      ]
    },
    "snap": {
      "artifactName": "digitales-klassenzimmer.${ext}",
      "plugs": [
        "default",
        "camera",
        "audio-record",
        "screen-inhibit-control"
      ]
    },
    "deb": {
      "depends": [
        "libgtk-3-0",
        "libnss3",
        "libxtst6",
        "xdg-utils",
        "libatspi2.0-0",
        "libuuid1"
      ]
    },
    "win": {
      "artifactName": "digitales-klassenzimmer-${version}-${arch}.${ext}",
      "publish": {
        "provider": "github",
        "publishAutoUpdate": false
      },
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "appx"
        },
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "nsis"
        }
      ]
    },
    "appx": {
      "identityName": "HoppFoundation.DigitalesKlassenzimmer",
      "displayName": "Digitales Klassenzimmer",
      "publisher": "CN=18816B78-05D2-489F-BF0C-51732BBF65E3",
      "publisherDisplayName": "Hopp Foundation",
      "languages": [
        "de-DE",
        "en-US"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "protocols": [
      {
        "name": "jitsi-protocol",
        "role": "Viewer",
        "schemes": [
          "jitsi-meet"
        ]
      }
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HoppFoundation/jitsi-meet-electron"
  },
  "keywords": [
    "jingle",
    "webrtc",
    "xmpp",
    "electron",
    "hopp foundation"
  ],
  "author": "Hopp Foundation <jitsi@hopp-foundation.de>",
  "readmeFilename": "README.md",
  "license": "Apache-2.0",
  "dependencies": {
    "@jitsi/electron-sdk": "^3.0.6",
    "electron-debug": "^3.2.0",
    "electron-reload": "^1.5.0"
  },
  "devDependencies": {
    "@atlaskit/button": "^10.1.3",
    "@atlaskit/css-reset": "^3.0.8",
    "@atlaskit/droplist": "^7.0.19",
    "@atlaskit/field-text": "^7.1.0",
    "@atlaskit/icon": "^15.0.3",
    "@atlaskit/navigation": "^33.3.10",
    "@atlaskit/onboarding": "^6.2.0",
    "@atlaskit/page": "^8.0.12",
    "@atlaskit/spinner": "^9.0.13",
    "@atlaskit/theme": "^7.0.5",
    "@atlaskit/toggle": "^5.0.15",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-transform-flow-strip-types": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@jitsi/js-utils": "2.0.0",
    "@svgr/webpack": "^6.2.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.2.3",
    "concurrently": "5.1.0",
    "css-loader": "^6.7.1",
    "electron": "19.0.6",
    "electron-builder": "23.1.0",
    "electron-context-menu": "^2.5.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.2",
    "electron-notarize": "1.1.1",
    "electron-react-devtools": "0.5.3",
    "electron-updater": "5.0.5",
    "electron-window-state": "^5.0.3",
    "eslint": "6.5.1",
    "eslint-config-jitsi": "github:jitsi/eslint-config-jitsi#1.0.2",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "22.1.0",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "^6.2.0",
    "flow-bin": "0.109.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.0",
    "i18next": "^19.9.2",
    "moment": "^2.29.2",
    "mousetrap": "^1.6.5",
    "patch-package": "6.2.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.8.12",
    "react-redux": "^5.1.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^3.3.1",
    "styled-components": "^3.4.10",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
